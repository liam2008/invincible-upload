<!-- Wrapper-->
<div id="wrapper">

    <!-- Navigation -->
    <div ng-include="'views/navigation.html'|version"></div>

    <!-- Page wraper -->
    <!-- ng-class with current state name give you the ability to extended customization your view -->
    <div id="page-wrapper" class="gray-bg {{$state.current.name}}">

        <!-- Page wrapper -->
        <div ng-include="'views/topnavbar.html'|version"></div>

        <div class="row wrapper border-bottom white-bg page-heading">
            <div class="col-lg-10">
                <h2>{{navigatorMenuItem}}</h2>
                <ol class="breadcrumb">-
                    <li>
                        <a ui-sref="main.summary">首页</a>
                    </li>
                    <li ng-if="navigatorMenu">
                        <a>{{navigatorMenu}}</a>
                    </li>
                    <li class="active">
                        <strong>{{navigatorMenuItem}}</strong>
                    </li>
                </ol>
            </div>
            <div class="col-lg-2">

            </div>
        </div>

        <!-- Main view  -->
        <div ui-view></div>

        <!-- Footer -->
        <div ng-include="'views/footer.html'|version"></div>

    </div>
    <!-- End page wrapper-->
</div>
<!-- End wrapper-->